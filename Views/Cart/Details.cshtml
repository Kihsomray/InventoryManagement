@model Cart

<h2>Cart Item Details</h2>
<br>
<div>
    <h4>Cart Item</h4>
    <hr />
    <dl class="row">
        <dt>Customer ID</dt>
        <dd>@Model.CustomerID</dd>
        <dt>Item ID</dt>
        <dd>@Model.ItemID</dd>
        <dt>Quantity</dt>
        <dd>@Model.Quantity</dd>
    </dl>
</div>
<p><a asp-action="Index">Back to Cart</a></p>
