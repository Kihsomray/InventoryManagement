@model Customer

<h2>Customer Details</h2>
<br>
<div>
    <h4>Customer</h4>
    <hr />
    <dl class="row">
        <dt>Customer ID</dt>
        <dd>@Model.CustomerID</dd>
        <dt>Full Name</dt>
        <dd>@Model.FullName</dd>
        <dt>Email</dt>
        <dd>@Model.Email</dd>
        <dt>Phone Number</dt>
        <dd>@Model.PhoneNumber</dd>
        <dt>Address</dt>
        <dd>@Model.Address</dd>
        <dt>Date Of Creation</dt>
        <dd>@Model.DateOfCreation</dd>
    </dl>
</div>
<p><a asp-action="Index">Back to List</a></p>
